{
    "version": 2,
    "builds": [
        {
            "src": "backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdasize": "15mb",
                "runtime": "python3"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1D"
      },
      {
        "src": "/(.*)",
        "dest": "timesparkBackend/wsgi.py"
      }
    ]
  }
  